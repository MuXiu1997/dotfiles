[user]
    name = {{ .name }}
    email = {{ .email }}
    {{ if has "personal" .tags -}}
    signingkey = FF066B943A937BD1
    {{- end }}
[init]
    defaultBranch = main
[core]
    autocrlf = input
    ignorecase = false
[include]
    path = ../delta/config
[push]
    default = current
[commit]
    {{ if has "personal" .tags -}}
    gpgsign = true
    {{- end }}
[log]
    date = format:%Y-%m-%d %H:%M:%S
[safe]
    directory = {{ .homebrewPrefix }}
[includeIf "gitdir:@kanyun.com/"]
    path = kanyun.config
