# { Homebrew
    if type brew &>/dev/null; then
        eval "$(brew shellenv)"
        export FPATH="$FPATH:$HOMEBREW_PREFIX/share/zsh/site-functions"
    fi
# }

# { Pyenv
    if type pyenv &>/dev/null; then
        eval "$(pyenv init --path)"
    fi
# }

# { Update completions
    if type update-completions &>/dev/null; then
        update-completions
    fi
# }

