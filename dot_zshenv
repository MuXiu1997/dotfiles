# { EDITOR
    if type nvim &>/dev/null; then
        export EDITOR='nvim'
    else
        export EDITOR='vim'
    fi
# }

# { Ranger
    export RANGER_LOAD_DEFAULT_RC="FALSE"
# {

export FPATH="$HOME/.config/zsh/completions:$FPATH"

export PATH="$HOME/bin:$PATH"
# Brew
export PATH="/opt/homebrew/bin:$PATH"

